{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"airbnb-base",
		"prettier"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"prettier"
	],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5"
			}
		]
	}
}